{
  "name": "@scalar/swagger-editor",
  "version": "0.0.0",
  "author": "Scalar",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "vite build && pnpm types:build && tsc-alias -p tsconfig.build.json",
    "dev": "vite",
    "lint:check": "eslint .",
    "lint:fix": "eslint .  --fix",
    "preview": "vite preview",
    "test:unit": "vitest ./src --coverage",
    "types:build": "vue-tsc -p tsconfig.build.json",
    "types:check": "vue-tsc --noEmit --skipLibCheck"
  },
  "type": "module",
  "main": "src/index.ts",
  "dependencies": {
    "@anc/library": "workspace:*",
    "@codemirror/lang-json": "6.0.1",
    "@codemirror/language": "6.8.0",
    "@codemirror/legacy-modes": "6.3.3",
    "@codemirror/state": "6.2.1",
    "@codemirror/view": "6.16.0",
    "@hocuspocus/common": "2.3.1",
    "@hocuspocus/provider": "2.3.1",
    "@uiw/codemirror-theme-duotone": "4.21.3",
    "@vueuse/core": "10.1.2",
    "codemirror": "6.0.1",
    "y-codemirror.next": "0.3.2",
    "yjs": "13.6.7"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "4.2.3",
    "tsc-alias": "1.8.6",
    "vite": "4.4.8",
    "vitest": "0.34.1",
    "vue-tsc": "1.8.5"
  },
  "peerDependencies": {
    "vue": "3.3.4"
  }
}
