  },
  "devDependencies": {
    "@vitejs/plugin-vue": "4.2.3",
    "tsc-alias": "1.8.6",
    "vite": "4.4.8",
    "vitest": "0.34.1",
    "vue-tsc": "1.8.8"
  },
  "peerDependencies": {
    "vue": "3.3.4"
  }
